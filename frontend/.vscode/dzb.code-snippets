  "genHooksPaginationNew": {
    "prefix": "genHooksPagination",
    "body": [
      "import { useEffect, useState, useCallback, useRef } from \"react\";",
      "import { cloneDeep, isEmpty, isNumber } from \"lodash\";",
      "import { useSave } from \"@/stores/useStores\";",
      "import * as moment from \"moment\";",
      "",
      "import { showError } from \"@/helpers/toast\";",
      "import OrderServices from \"services/Order.service\";",
      "import { convertParamFilter } from \"@/helpers/common\";",
      "import { DateTimeFormat } from \"@/consts/dates\";",
      "import { errorHandler } from \"@/helpers/errors\";",
      "import { I$1, IRequestGet$1, IResponse$1 } from \"interfaces/$1.interface\";",
      "",
      "/**",
      " * Please check:",
      " * - services in fetch()",
      " * - services in refetch()",
      " * - parseRequest()",
      " * - checkConditionPass()",
      " * @param options",
      " * @returns",
      " */",
      "",
      "//* Check parse body request",
      "const parseRequest = (filters?: IRequestGet$1) => {",
      "  return cloneDeep({",
      "    page: filters?.page,",
      "    page_size: filters?.page_size,",
      "    start_date: filters?.start_date ? moment(filters?.start_date).format(DateTimeFormat.APIFormat) : null,",
      "    end_date: filters?.end_date ? moment(filters?.end_date).format(DateTimeFormat.APIFormat) : null,",
      "    customer: filters?.customer ? filters?.customer : null,",
      "    status: filters?.status ? filters?.status : null,",
      "  });",
      "};",
      "",
      "const useGet$1 = (",
      "  filters?: IRequestGet$1,",
      "  options: { isTrigger?: boolean; cachedKey?: string } = { isTrigger: true, cachedKey: \"\" }",
      ") => {",
      "  //! State",
      "  const signal = useRef(new AbortController());",
      "  const { isTrigger = true, cachedKey = \"\" } = options;",
      "",
      "  const save = useSave();",
      "  const [data, setData] = useState<I$1>();",
      "  const [isLoading, setLoading] = useState(false);",
      "  const [isRefetching, setRefetching] = useState(false);",
      "  const [isFetchingPage, setFetchingPage] = useState(false);",
      "  const [error, setError] = useState<unknown>(null);",
      "  const [hasMore, setHasMore] = useState(false);",
      "",
      "  //! Function",
      "  const fetch: () => Promise<IResponse$1> | undefined = useCallback(() => {",
      "    if (!isTrigger) {",
      "      return;",
      "    }",
      "",
      "    return new Promise((resolve, reject) => {",
      "      (async () => {",
      "        try {",
      "          const nextFilters = convertParamFilter(parseRequest, filters);",
      "          const response = await OrderServices.getOrderList(nextFilters, {",
      "            signal: signal.current.signal,",
      "          });",
      "          resolve(response);",
      "        } catch (error) {",
      "          setError(error);",
      "          reject(error);",
      "          setFetchingPage(false);",
      "        }",
      "      })();",
      "    });",
      "  }, [filters, isTrigger]);",
      "",
      "  const checkConditionPass = useCallback(",
      "    (response: IResponse$1) => {",
      "      //* Check condition of response here to set data",
      "      if (!isEmpty(response?.data)) {",
      "        setData(response.data);",
      "",
      "        const total_pages = response?.data?.total_pages;",
      "        if (isNumber(filters?.page)) {",
      "          setHasMore(filters?.page < total_pages);",
      "        }",
      "      }",
      "    },",
      "    [filters?.page]",
      "  );",
      "",
      "  //* Refetch implicity (without changing loading state)",
      "  const refetch = useCallback(async () => {",
      "    try {",
      "      setRefetching(true);",
      "      signal.current = new AbortController();",
      "      const nextFilters = parseRequest(filters);",
      "      const response = await OrderServices.getOrderList(nextFilters, { signal: signal.current.signal });",
      "      if (response) {",
      "        checkConditionPass(response);",
      "      }",
      "    } catch (error: any) {",
      "      const msgError = errorHandler(error);",
      "      if (msgError) {",
      "        showError(error);",
      "      }",
      "    } finally {",
      "      setRefetching(false);",
      "    }",
      "  }, [filters]);",
      "",
      "  useEffect(() => {",
      "    if (cachedKey) {",
      "      save(cachedKey, {",
      "        refetch,",
      "        data,",
      "        isLoading,",
      "        hasMore,",
      "      });",
      "    }",
      "",
      "    return () => {",
      "      save(cachedKey, undefined);",
      "    };",
      "  }, [save, cachedKey, isLoading, refetch, data, hasMore]);",
      "",
      "  //* Refetch with changing loading state",
      "  const refetchWithLoading = useCallback(",
      "    async (shouldSetData: boolean) => {",
      "      try {",
      "        setLoading(true);",
      "        const response = await fetch();",
      "        if (shouldSetData && response) {",
      "          checkConditionPass(response);",
      "        }",
      "        setLoading(false);",
      "      } catch (error) {",
      "        setError(error);",
      "      } finally {",
      "        setLoading(false);",
      "      }",
      "    },",
      "    [fetch, checkConditionPass]",
      "  );",
      "",
      "  useEffect(() => {",
      "    let shouldSetData = true;",
      "    signal.current = new AbortController();",
      "",
      "    if (filters?.page !== undefined && filters?.page <= 1) {",
      "      (async () => {",
      "        try {",
      "          setLoading(true);",
      "          const response = await fetch();",
      "          if (shouldSetData && response) {",
      "            checkConditionPass(response);",
      "          }",
      "        } catch (error) {",
      "          console.log(\"ERROR / Initial fetching / \", error);",
      "          setError(error);",
      "        } finally {",
      "          setLoading(false);",
      "        }",
      "      })();",
      "    } else {",
      "      //* If page > 1 -> fetch more page",
      "      (async () => {",
      "        try {",
      "          setFetchingPage(true);",
      "          const response = await fetch();",
      "          if (shouldSetData && response) {",
      "            checkConditionPass(response);",
      "          }",
      "        } catch (error) {",
      "          console.log(\"ERROR / Fetching page / \", error);",
      "          setError(error);",
      "        } finally {",
      "          setFetchingPage(false);",
      "        }",
      "      })();",
      "    }",
      "",
      "    return () => {",
      "      shouldSetData = false;",
      "      signal.current.abort();",
      "    };",
      "  }, [filters?.page, fetch]);",
      "",
      "  return {",
      "    data,",
      "    isLoading,",
      "    error,",
      "    refetch,",
      "    refetchWithLoading,",
      "    isRefetching,",
      "    isFetchingPage,",
      "    hasMore,",
      "    setData,",
      "  };",
      "};",
      "",
      "export default useGet$1;",
      ""
    ],
    "description": "genHooksPagination"
  },
  "genHookDetail": {
    "prefix": "genHookDetail",
    "body": [
      "import { useRef, useEffect, useState, useCallback } from \"react\";",
      "import { isEmpty } from \"lodash\";",
      "import { useSave } from \"@/store/useStores\";",
      "",
      "import { showError } from \"@/helpers/toast\";",
      "import { errorHandler } from \"@/helpers/errors\";",
      "import $1Services from \"services/$1.service\";",
      "import { ResponseGet$1Detail, $1 } from \"interfaces/$1.interface\";",
      "",
      "/**",
      " * Please check:",
      " * - fetch()",
      " * - refetch()",
      " * - checkConditionPass()",
      " */",
      "const useGet$1Detail = (",
      "  id: string,",
      "  options: { isTrigger?: boolean; cachedKey?: string } = { isTrigger: true, cachedKey: \"\" }",
      ") => {",
      "  //! State",
      "  const signal = useRef(new AbortController());",
      "  const { isTrigger = true, cachedKey = \"\" } = options;",
      "",
      "  const save = useSave();",
      "  const [data, setData] = useState<$1>();",
      "  const [isLoading, setLoading] = useState(false);",
      "  const [isRefetching, setRefetching] = useState(false);",
      "  const [error, setError] = useState<unknown>(null);",
      "",
      "  //! Function",
      "  const fetch: () => Promise<ResponseGet$1Detail> | undefined = useCallback(() => {",
      "    if (!isTrigger) {",
      "      return;",
      "    }",
      "",
      "    return new Promise((resolve, reject) => {",
      "      (async () => {",
      "        try {",
      "          const response = await $1Services.get$1Detail(id, { signal: signal.current.signal });",
      "          resolve(response);",
      "        } catch (error) {",
      "          setError(error);",
      "          reject(error);",
      "        }",
      "      })();",
      "    });",
      "  }, [id, isTrigger]);",
      "",
      "  const checkConditionPass = useCallback((response: ResponseGet$1Detail) => {",
      "    //* Check condition of response here to set data",
      "    if (!isEmpty(response?.data)) {",
      "      setData(response.data);",
      "    }",
      "  }, []);",
      "",
      "  //* Refetch implicity (without changing loading state)",
      "  const refetch = useCallback(async () => {",
      "    try {",
      "      setRefetching(true);",
      "      signal.current = new AbortController();",
      "      const response = await $1Services.get$1Detail(id, { signal: signal.current.signal });",
      "      checkConditionPass(response);",
      "    } catch (error: any) {",
      "      showError(errorHandler(error));",
      "    } finally {",
      "      setRefetching(false);",
      "    }",
      "  }, [id]);",
      "",
      "  useEffect(() => {",
      "    if (cachedKey) {",
      "      save(cachedKey, { refetch, data, isLoading, isRefetching });",
      "    }",
      "  }, [save, cachedKey, refetch, data, isLoading, isRefetching]);",
      "",
      "  //* Refetch with changing loading state",
      "  const refetchWithLoading = useCallback(",
      "    async (shouldSetData: boolean) => {",
      "      try {",
      "        setLoading(true);",
      "        signal.current = new AbortController();",
      "        const response = await fetch();",
      "        if (shouldSetData && response) {",
      "          checkConditionPass(response);",
      "        }",
      "      } catch (error) {",
      "        setError(error);",
      "      } finally {",
      "        setLoading(false);",
      "      }",
      "    },",
      "    [fetch, checkConditionPass]",
      "  );",
      "",
      "  useEffect(() => {",
      "    let shouldSetData = true;",
      "    signal.current = new AbortController();",
      "",
      "    (async () => {",
      "      try {",
      "        setLoading(true);",
      "        const response = await fetch();",
      "        if (shouldSetData && response) {",
      "          checkConditionPass(response);",
      "        }",
      "      } catch (error) {",
      "        setError(error);",
      "      } finally {",
      "        setLoading(false);",
      "      }",
      "    })();",
      "",
      "    return () => {",
      "      shouldSetData = false;",
      "      signal.current.abort();",
      "    };",
      "  }, [fetch, checkConditionPass]);",
      "",
      "  return {",
      "    data,",
      "    isLoading,",
      "    error,",
      "    refetch,",
      "    refetchWithLoading,",
      "    isRefetching,",
      "    setData,",
      "  };",
      "};",
      "",
      "export default useGet$1Detail;",
      ""
